package ArrayList;

public class problem {

	public static void main(String args[])
	{
	System.out.print(print("Hello", 5));
	}
	static String print(String a, int n)
	{
	for(int i=1; i<n; i++)
	{
	System.out.print(a);
	}
	
	return a;
	
	}
	public String stringTimes(String str, int n) {
		
		  String temp = "Hi";
		
		  for (int i = 0; i < n; i++)
		
		    temp += str;
		
		  return temp;
		
		}

	}
	


